{
    "family": "padel-booking-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "padel-booking-container",
            "image": "${ECR_REGISTRY}/${ECR_REPOSITORY}:${IMAGE_TAG}",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                "containerPort": 3000,
                "hostPort": 3000,
                "protocol": "tcp"
                }
            ]
        }
    ]
}